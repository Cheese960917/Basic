import java.util.Scanner;
public class Prime {

	public int sumofPrime {
		// TODO Auto-generated method stub
		Scanner sc = new Scanner(System.in);
		int n = sc.nextInt();
		int res = 2;
		if(n < 2) {
			return 0;
		}
		outer:
		for(int i = 3; i <= n ; i++) {
			for(int j = i - 1; j > 1; j--) {
				if( i % j == 0) {
					continue outer;
				}
				if( i % j != 0 && j == 2) {
					res += i;
				}
			}
		}
		return res;
	}
}
